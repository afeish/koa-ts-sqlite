{
  "name": "@neocxf/dappstore",
  "version": "1.0.0",
  "description": "starter kit for developing koajs with typescript",
  "main": "dist/index",
  "typings": "types/index.d.ts",
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "server": "nodemon dist",
    "dev": "npm-run-all clean build  db --parallel server build:watch ",
    "prod": "npm-run-all clean build db server",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "clean": "rimraf -rf dist"
  },
  "keywords": [],
  "author": "neo",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.1",
    "koa-body": "^4.0.4",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "mz": "^2.7.0",
    "nodemon": "1.11.0",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.30",
    "@types/mocha": "2.2.41",
    "@types/mz": "0.0.32",
    "@types/node": "7.0.22",
    "@types/sequelize": "^4.27.23",
    "@types/sqlite3": "^3.1.3",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.6.0",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "mocha": "3.4.2",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sequelize-cli": "^4.0.0",
    "supertest": "3.0.0",
    "typescript": "2.9.2",
    "typescript-eslint-parser": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocxf/node-ts.git"
  },
  "bugs": {
    "url": "https://github.com/neocxf/node-ts/issues"
  },
  "homepage": "https://github.com/neocxf/node-ts#readme"
}
